using System;

interface IVolador  // Interface
{
    void Volar();
}

class Pajaro : IVolador  // Clase que implementa la interfaz
{
    public void Volar()
    {
        Console.WriteLine("El pájaro está volando.");
    }
}

class Program
{
    static void Main()
    {
        Pajaro miPajaro = new Pajaro();
        miPajaro.Volar();  // Llamar al método de la interfaz
    }
}
